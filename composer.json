{
    "name": "SocialEngine/sniffer-rules",
    "description": "SnifferRules detect violations of a defined coding standard. It helps your code remains clean and consistent.",
    "keywords": ["sniff","psr2","psr1"],
    "authors": [
        {
            "name": "SocialEngine"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4.0",
        "illuminate/console": "~4.0",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "SocialEngine\\SnifferRules\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
